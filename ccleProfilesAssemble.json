{
  "pipeline": {
    "name": "ccleProfilesAssemble"
  },
  "transform": {
    "image": "bhklab/pharmacogx:v3",
    "cmd": [
      "Rscript", 
      "/pfs/ccleProfiles/assembleData.R"]
    },
  "parallelism_spec":{
    "constant": 1
  },
  "resource_requests": {
    "memory": "0.5G"
  },
  "input": {
    "cross": [
      {
        "git": {
          "url": "https://github.com/BHKLAB-Pachyderm/ccleProfiles.git",
          "name": "",
          "branch": "master"
        }
      },
      {
        "pfs": {
          "repo": "calculateccleRAW",
          "name": "input",
          "glob": "/"
        }
      } 
    ]
  }
}
